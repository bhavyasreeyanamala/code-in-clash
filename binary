import java.util.*;
import java.io.*;
import java.math.*;
class Solution {

    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int a=1;
        for(int i=1;i<=n;i++){
          a*=2;
        }
        System.out.println(a-1);
    }
}
